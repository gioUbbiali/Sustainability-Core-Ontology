[ rdf:type owl:Axiom ;
   owl:annotatedSource sco-u:SCO_0000059 ;
   owl:annotatedProperty rdfs:domain ;
   owl:annotatedTarget <http://www.ontologyrepository.com/CommonCoreOntologies/InformationContentEntity> ;
